{
  "private": true,
  "scripts": {
    "build": "rocket-punch build",
    "publish": "rocket-punch publish",
    "test": "jest --colors",
    "coverage": "jest --coverage --colors",
    "format": "prettier --write \"src/{**/*,*}/*.{ts,tsx}\"",
    "lint": "eslint \"src/{**/*,*}/*.{ts,tsx}\"",
    "docs:import": "markdown-source-import \"{,!(node_modules)/**/}*.md\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "markdown-source-import \"{,!(node_modules)/**/}*.md\" --git-add && lint-staged"
    }
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "jest --colors --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@ssen/eslint-config": "^1.1.0",
    "@ssen/prettier-config": "^1.0.0",
    "@types/eslint": "^7.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.4",
    "@types/mongodb": "^3.5.25",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.0.2",
    "date-fns": "^2.14.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "fs-extra": "^9.0.1",
    "google-spreadsheet": "^3.0.11",
    "googleapis": "^54.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "markdown-source-import": "^1.3.1",
    "mongodb": "^3.5.9",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "rocket-punch": "^1.0.1",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0"
  }
}
